  <string name="ten_gb">10 GB</string>
  <!-- Choice for the user to select a maximum trace size. -->
  <string name="twenty_gb">20 GB</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="ten_minutes">10 នាទី</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="thirty_minutes">30 នាទី</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="one_hour">1 ម៉ោង</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="eight_hours">8 ម៉ោង</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="twelve_hours">12 ម៉ោង</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="twentyfour_hours">24 ម៉ោង</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="four_thousand_kb">4096 KB</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="eight_thousand_kb">8192 KB</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="sixteen_thousand_kb">16384 KB</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="twentyfour_thousand_kb">24576 KB</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="thirtytwo_thousand_kb">32768 KB</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="sixtyfive_thousand_kb">65536 KB</string>
  <!-- This is the label for a picker that will let the user select the cpu mode. -->
  <string name="perf_cpu_mode">Perf_CPU Mode</string>
  <!-- Choice for the user to select cpu mode. -->
  <string name="performance_mode">ប្រតិបត្តិការ</string>
  <string name="equalization_mode_1">balance_1</string>
