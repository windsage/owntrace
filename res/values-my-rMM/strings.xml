  <!-- Choice for the user to select a maximum trace size. -->
  <string name="ten_gb">10 ဂစ်ဂါဘိုက်</string>
  <!-- Choice for the user to select a maximum trace size. -->
  <string name="twenty_gb">20 ဂစ်ဂါဘိုက်</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="ten_minutes">10 မိနစ်</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="thirty_minutes">30 မိနစ်</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="one_hour">1 နာရီ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="eight_hours">8 နာရီ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="twelve_hours">12 နာရီ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="twentyfour_hours">24 နာရီ</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="four_thousand_kb">4096 ကီလိုဘိုက်</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="eight_thousand_kb">8192 ကီလိုဘိုက်</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="sixteen_thousand_kb">16384 ကီလိုဘိုက်</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="twentyfour_thousand_kb">24576 ကီလိုဘိုက်</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="thirtytwo_thousand_kb">32768 ကီလိုဘိုက်</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="sixtyfive_thousand_kb">65536 ကီလိုဘိုက်</string>
  <!-- This is the label for a picker that will let the user select the cpu mode. -->
  <string name="perf_cpu_mode">Perf_CPU မုဒ်</string>
  <!-- Choice for the user to select cpu mode. -->
  <string name="performance_mode">စွမ်းဆောင်ရည်</string>
  <string name="equalization_mode_1">balance_1</string>
  <string name="equalization_mode_2">balance_2</string>
