  <string name="ten_minutes">10 منٹ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="thirty_minutes">30 منٹس</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="one_hour">1 گھنٹٕہ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="eight_hours">8 گھنٹہ</string>
  <!-- Choice for the user to select a maximum trace time. -->
