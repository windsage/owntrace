  <string name="ten_minutes">10 মিনিট</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="thirty_minutes">30 মিনিট</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="one_hour">1 ঘন্টা</string>
