  <!-- Choice for the user to select a maximum trace size. -->
  <string name="twenty_gb">20 ਜੀਬੀ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="ten_minutes">10 ਮਿੰਟ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="thirty_minutes">30 ਮਿੰਟ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="one_hour">1 ਘੰਟਾ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="eight_hours">8 ਘੰਟੇ</string>
  <!-- Choice for the user to select a maximum trace time. -->
