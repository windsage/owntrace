  <string name="ten_minutes">10 മിനിറ്റ്</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="thirty_minutes">30 മിനിറ്റ്</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="one_hour">1 മണിക്കൂർ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="eight_hours">8 മണിക്കൂർ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="twelve_hours">12 മണിക്കൂർ</string>
  <!-- Choice for the user to select a maximum trace time. -->
  <string name="twentyfour_hours">24 മണിക്കൂർ</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="four_thousand_kb">4096 KB</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="eight_thousand_kb">8192 KB</string>
  <!-- Choice for the user to select a maximum trace buffer size. -->
  <string name="sixteen_thousand_kb">16384 KB</string>
